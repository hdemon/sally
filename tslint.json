{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": [true],
    "no-var-keyword": true,
    "prefer-const": true,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "src/@types/**/*"
    ]
  }
}
